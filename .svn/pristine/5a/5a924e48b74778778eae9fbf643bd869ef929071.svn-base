package fr.eseo.atribus.entities;

import java.io.Serializable;
import java.util.List;

public class Examen implements Serializable {

  private static final long serialVersionUID = -6123285159144541101L;

  private int id;
  private Matiere matiere;
  private List<Exercice> exercices;
  private String nom;
  private Boolean autoEvaluation;

  public int getId() {
    return this.id;
  }

  public void setId(int id) {
    this.id = id;
  }

  public Matiere getMatiere() {
    return this.matiere;
  }

  public void setMatiere(Matiere matiere) {
    this.matiere = matiere;
  }

  public List<Exercice> getExercices() {
    return this.exercices;
  }

  public void setExercices(List<Exercice> exercices) {
    this.exercices = exercices;
  }

  public String getNom() {
    return this.nom;
  }

  public void setNom(String nom) {
    this.nom = nom;
  }

  public Boolean getAutoEvaluation() {
    return this.autoEvaluation;
  }

  public void setAutoEvaluation(Boolean autoEvaluation) {
    this.autoEvaluation = autoEvaluation;
  }

  @Override
  public String toString() {
    return "Examen [id=" + this.id + ", matiere=" + this.matiere + ", exercices=" + this.exercices
        + ", nom=" + this.nom + ", autoEvaluation=" + this.autoEvaluation + "]";
  }

}
